{
  "name": "ulasis---customer-intelligence-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:comprehensive": "vitest run --run tests/comprehensive-error-detection.test.tsx",
    "test:build-validation": "vitest run --run tests/build-validation.test.ts",
    "test:runtime-errors": "vitest run --run tests/runtime-error-detection.test.tsx",
    "test:e2e-errors": "playwright test e2e/error-detection.spec.ts",
    "test:all": "npm run test:comprehensive && npm run test:build-validation && npm run test:runtime-errors && npm run test:e2e-errors",
    "check:errors": "scripts\\comprehensive-error-detection.bat",
    "check:errors:unix": "bash scripts/comprehensive-error-detection.sh"
  },
  "dependencies": {
    "@google/genai": "^1.24.0",
    "@types/qrcode": "^1.5.6",
    "axios": "^1.13.2",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "recharts": "^3.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tanstack/react-query": "^5.90.7",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "autoprefixer": "^10.4.21",
    "jsdom": "^27.1.0",
    "msw": "^2.12.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.7",
    "vitest-canvas-mock": "^0.3.3"
  }
}
